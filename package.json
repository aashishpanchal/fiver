{
  "name": "ufiber",
  "version": "0.0.0-exp-2",
  "type": "module",
  "main": "./dist/index.d.cjs",
  "module": "./dist/index.d.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "bin/install.js"
  ],
  "scripts": {
    "build": "pnpm remove-dist && node build.ts",
    "build:watch": "pnpm remove-dist && node build.ts --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,js}\" --fix",
    "test": "vitest run",
    "test:types": "vitest --typecheck --run --passWithNoTests",
    "format": "prettier --write \"./{src,tests}/**/*.{ts,js}\"",
    "release": "pnpm run build && npm publish",
    "pre-commit": "pnpm format && pnpm test",
    "check-types": "tsc --noEmit",
    "coverage": "vitest --run --coverage",
    "remove-dist": "rm -rf dist",
    "prepare": "husky",
    "postinstall": "node bin/install.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs"
    },
    "./uws": {
      "import": "./uws/ESM_wrapper.mjs",
      "types": "./uws/index.d.ts",
      "require": "./uws/uws.js"
    }
  },
  "typesVersions": {
    "*": {
      "uws": [
        "./uws/index.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "arg": "^5.0.2",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsdown": "^0.15.12",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "unplugin-unused": "^0.5.6",
    "vitest": "^4.0.8"
  },
  "license": "MIT",
  "author": "Aashish Panchal <aipanchal51@gmail.com>",
  "description": "Next-gen webserver for node-js developer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aashishpanchal/ufiber.git"
  },
  "homepage": "https://github.com/aashishpanchal/ufiber#readme",
  "keywords": [
    "node",
    "http",
    "web",
    "server",
    "uws",
    "Fiber"
  ],
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.20.0"
}
